{
  "manifest_version": 3,
  "name": "Fake Filler",
  "version": "1.0",
  "description": "Fills all form inputs with dummy data for testing purposes.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  }
}